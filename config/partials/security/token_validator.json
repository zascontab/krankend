{
  "validator": "custom",
  "validator_url": "http://token-service:8081/api/v1/validate",
  "method": "POST",
  "body_encoding": "json",
  "cache": true,
  "cache_duration": 3600,
  "header_name": "Authorization",
  "header_value_schema": "Bearer {jwt}",
  "forward": {
    "header": {
      "X-User-ID": "sub",
      "X-User-Email": "email",
      "X-User-Name": "name",
      "X-User-Roles": "realm_access.roles"
    }
  }
}