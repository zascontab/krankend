{
  "alg": "RS256",
  "jwk_url": "{{ .service_hosts.iam }}/jwks.json",
  "cache": true,
  "cache_duration": 3600,
  "disable_jwk_security": {{ if eq .environment "development" }}true{{ else }}false{{ end }},
  "propagate_claims": [
    ["sub", "x-user-id"],
    ["realm_access.roles", "x-user-roles"]
  ]
}